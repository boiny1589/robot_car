<launch>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <rosparam file="$(find nav_ma)/param/param_senior_mec_no/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find nav_ma)/param/param_senior_mec_no/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find nav_ma)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find nav_ma)/param/global_costmap_params.yaml" command="load" />
        <!-- <rosparam file="$(find nav_ma)/param/base_local_planner_params.yaml" command="load" /> -->
        <rosparam file="$(find nav_ma)/param/origin/move_base_params.yaml" command="load" />
        <rosparam file="$(find nav_ma)/param/origin/base_global_planner_params.yaml" command="load" />
        <rosparam file="$(find nav_ma)/param/origin/teb_local_planner_params.yaml" command="load" />
    </node>
    
</launch>